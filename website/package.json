        "test:lint": "eslint \"./**/*.{ts,tsx,js,jsx}\" && stylelint ./**/*.scss",
        "test:lint:fix": "eslint --fix './**/*.{ts,tsx,js,jsx}' && stylelint **/*.scss --fix",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "docusaurus write-translations",
        "write-heading-ids": "docusaurus write-heading-ids",
        "contributors:add": "all-contributors add",
        "contributors:generate": "all-contributors generate"
    },
    "dependencies": {
        "@ant-design/icons": "^4.6.2",
        "@docusaurus/core": "2.0.0-beta.8",
        "@docusaurus/plugin-client-redirects": "2.0.0-beta.8",
        "@docusaurus/plugin-ideal-image": "2.0.0-beta.8",
        "@docusaurus/preset-classic": "2.0.0-beta.8",
        "@mdx-js/react": "^1.6.21",
        "@svgr/webpack": "^5.5.0",
        "clsx": "^1.1.1",
        "docusaurus-plugin-hotjar": "^0.0.2",
        "dotenv": "^10.0.0",
        "file-loader": "^6.2.0",
        "js-cookie": "^3.0.1",
        "react": "^17.0.1",
        "react-cookie-consent": "^6.4.1",
        "react-dom": "^17.0.1",
        "url-loader": "^4.1.1"
